# Bybit API キーのテンプレート（秘密は .env にのみ書くこと）
KEYS__API_KEY=your_bybit_key
KEYS__API_SECRET=your_bybit_secret
# 以下は任意（必要なら .env に記載し、YAMLより優先されます）
EXCHANGE__ENVIRONMENT=testnet
RISK__MAX_TOTAL_NOTIONAL=10000
RISK__MAX_SYMBOL_NOTIONAL=5000
RISK__MAX_NET_DELTA=0.001
RISK__MAX_SLIPPAGE_BPS=10
RISK__LOSS_CUT_DAILY_JPY=20000
STRATEGY__SYMBOLS=BTCUSDT,ETHUSDT   # カンマ区切りは単一文字列として渡る（必要に応じて YAML 側を推奨）
STRATEGY__MIN_EXPECTED_APR=0.05
STRATEGY__PRE_EVENT_OPEN_MINUTES=60
STRATEGY__HOLD_ACROSS_EVENTS=true
STRATEGY__REBALANCE_BAND_BPS=5.0
DB_URL=sqlite+aiosqlite:///./db/trading.db
TIMEZONE=UTC


